{
  "name": "ghostify",
  "description": "Export a Tumblr blog to Ghost.",
  "version": "0.0.1",
  "author": "John Moose @jmooserific",
  "bin": {
    "ghostify": "./bin/run"
  },
  "bugs": "https://github.com/jmooserific/ghostify/issues",
  "dependencies": {
    "@oclif/command": "^1.5.15",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "@types/node-fetch": "^2.3.7",
    "node-fetch": "^3.1.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.12",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jmooserific/ghostify",
  "keywords": [
    "tumbr",
    "ghost",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ghostify",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jmooserific/ghostify",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --reporter=lcov --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
